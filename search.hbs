{{> head}}
<main class=wrap-page>
<article class="card">
<div class=card-text>
    <form method="GET" action="https://www.google.com/cse" target="_top">
      <div class="ampstart-input inline-block relative mb3">
        <input name="cx" type="hidden" value="{{@config.custom.searchId}}" />
        <input name="ie" type="hidden" value="UTF-8" />
        <center><input class=search-input type="search" placeholder="جستجو..." name="q" required></center>
        <center><input type="submit" value="جستجو"></center>
      </div>
    </form>
</div>
</article>
</main>
{{> footer}}
